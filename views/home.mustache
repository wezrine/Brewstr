									<option value="washington">Washington</option>
									<option value="west Virginia">West Virginia</option>
									<option value="wisconsin">Wisconsin</option>
									<option value="wyoming">Wyoming</option>
							</select>
						</div>

						<button class="button" id="homeSearchButton">Search</button>


					</div>
				</div>
			</div>
		</div>

	</div>

	<!-- Main Search Photo Slider -->
	<div class="container msps-container">

		<div class="main-search-photo-slider">
			<div class="msps-slider-container">
				<div class="msps-slider">
					<div class="item"><img src="images/beer1.jpg" class="item" title="Title 1"/></div>
					<div class="item"><img src="images/beer2.jpg" class="item" title="Title 1"/></div>
					<div class="item"><img src="images/beer3.jpg" class="item" title="Title 1"/></div>
				</div>
			</div>
		</div>

		<div class="msps-shapes" id="scene">
		</div>
	</div>



</div>


<!-- Content
================================================== -->



<!-- Listings -->
<div class="container margin-top-70">
	<div class="row">

		<div class="col-md-12">
			<h3 class="headline margin-top-75">
					<strong class="headline-with-separator">Your Breweries</strong>
			</h3>
		</div>

		<div class="col-md-12">
			<div class="simple-slick-carousel dots-nav">

			<!-- Listing Item -->
			{{#usersBreweries}}
			<div class="carousel-item">
				<a href="/brewery/{{breweryId}}" class="listing-item-container">
					<div class="listing-item">
						<img src="images/listing-item-01.jpg" alt="">
						
						<div class="listing-item-content">
							<h3>{{name}}</h3>
							<span>{{street}} {{city}}, {{state}}</span>
							<span class="tag home-type">{{type}}</span>
						</div>
						<form method="POST" action="/delete-brewery">
							<input type="hidden" value={{breweryId}} name="breweryId">
							<span class="like-icon liked">
								<button onclick="deleteBrewery(this.parentElement.parentElement)" class="like-icon"></button>
							</span>
						</form>
					</div>
				</a>
			</div>
			{{/usersBreweries}}
			<!-- Listing Item / End -->

			</div>
			
		</div>

	</div>
</div>
